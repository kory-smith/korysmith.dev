---
import ArticleLayout from "@/layouts/ArticleLayout.astro";
import { getEntry } from "astro:content";

const usesArticle = await getEntry("standalone", "uses");
const { Content } = await usesArticle.render();
---

<ArticleLayout
  title={usesArticle.data.title}
  description={usesArticle.data.description}
>
  <Content />
</ArticleLayout>
